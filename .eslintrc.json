{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": "off",
    "operator-linebreak": ["error", "after"],
    "implicit-arrow-linebreak": ["error", "below", "beside"]
  }
}
